{
  "name": "ChromeRepl",
  "description": "A remote read-eval-print-loop for Chrome.",
  "version": "0.2",
  "background_page": "background.html",
  "permissions": [
    "http://*/", "https://*/",
    "bookmarks", "chrome://favicon/", "contextMenus", "cookies",
    "geolocation", "history", "idle", "notifications",
    "tabs", "unlimitedStorage"
  ],
  "icons": {
    "48": "icon48.png"
  }
}